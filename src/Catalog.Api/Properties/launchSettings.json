{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:59822",
      "sslPort": 44338
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "catalog",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "CloudEms.Catalog.Api": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "catalog",
      "environmentVariables": {
        "EXHUB_CONFIG_BLOB_STORAGE_BLOB_NAME": "appsettings-catalogapi.json",
        "EXHUB_CONFIG_BLOB_STORAGE_CONNECTION_STRING": "",
        "EXHUB_CONFIG_BLOB_STORAGE_CONTAINER_NAME": "configuration",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/catalog",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:443;http://+:80",
        "ASPNETCORE_HTTPS_PORT": "44339"
      },
      "httpPort": 59823,
      "useSSL": true,
      "sslPort": 44339
    },
    "Azure Dev Spaces": {
      "commandName": "AzureDevSpaces",
      "launchBrowser": true
    }
  }
}