{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:62306",
      "sslPort": 44325
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "echo",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "Docker": {
        "commandName": "Docker",
        "launchBrowser": true,
        "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/echo",
        "environmentVariables": {
          "ASPNETCORE_URLS": "https://+:443;http://+:80",
          "ASPNETCORE_HTTPS_PORT": "44339"
        },
        "httpPort": 59823,
        "useSSL": true,
        "sslPort": 44339
      }
    },
    "echo": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "echo",
      "applicationUrl": "http://localhost:8081",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
